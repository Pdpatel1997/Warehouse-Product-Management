@model List<FirstAppRecall.Models.Category>
@{
    ViewBag.Title = "Categories";
}

<h2>Categoris</h2>
<table class="table">
    <tr>
        <th>Category ID</th>
        <th>Category Name</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CategoryID</td>
            <td>@item.CategoryName</td>
        </tr>
    }
</table>


